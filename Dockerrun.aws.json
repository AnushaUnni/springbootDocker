{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
    "name": "springbootmodule",
    "image": "anushaunni/springboot_module",
    "hostname": "springbootmodule",
    "essential": false,
    "portMappings": [{
      "hostPort": 9000,
      "containerPort": 9001
    }]
  },
    {
      "name": "nginxmodule",
      "image": "anushaunni/nginx_module",
      "hostname": "nginxmodule",
      "essential": true,
      "portMappings": [{
        "hostPort": 3000,
        "containerPort": 80
      }],
      "links": ["springbootmodule"]
    }
  ]
}